  <section style="background-color: #eee; margin-top: 4rem; ">
    <div class="container vh-100   py-5">
      <div class="row">
        <div class="col">
          <nav aria-label="breadcrumb" class="bg-light rounded-3 p-3 mb-4">
            <ol class="breadcrumb mb-0" style="display: flex;  justify-content: space-between;">
              {{! <li class="breadcrumb-item"><a href="/checkOut">Back</a></li> }}
              <li class="breadcrumb-item">
                <h4>Sales Report</h4>
              </li>
              <div>
                <form name="salesReport" onsubmit="return report()">
                  <label for="">From</label>
                  <input type="date" name="date1" required>&nbsp;&nbsp;
                  <label for="">To</label>
                  <input type="date" name="date2" required>&nbsp;
                  <input type="submit" style="background-color: rgb(16, 14, 14); color: white; font-family: cursive; "
                    value="Filter">
                </form>
              </div>
            </ol>
          </nav>
        </div>
      </div>



    <div style="overflow: auto;">
      <table id="table-id" class="table align-middle mb-0 bg-white container">
        
        
        <thead class="bg-light">
         <th style="border: none;  background-color: rgb(255, 255, 255);"> <h1 style="text-align: center; font-weight: bold; font-family: cursive;">kk foods</h1></th>
          <tr>
       <th style="border: none; background-color: rgb(255, 255, 255);"> <p id="date11">From:{{fromDate}}</p><p id="date22">To:{{toDate}}</p></th>
         <a type="button" style="color: blue;" onclick="downloadPDF()">download report</a>
        
        </tr>
          <tr>
            <th>Order ID</th>
            <th>Username</th>
            <th>Product</th>
            <th>price </th>
            <th>Qty</th>
            <th>Sub Total</th>



          </tr>
        </thead>
        {{!-- {{#if users}} --}}
        
        <tbody id="getReport">
          {{#each users}}


          <tr  >

            <td>

              <p class="fw-normal mb-1">{{this.orderid}}</p>
            </td>
            <td>
              <p class="fw-normal mb-1">{{this.name}} </p>
            </td>
            <td>
              <p class="fw-normal mb-1">{{this.productName}}</p>
            </td>
            <td>
              <p class="fw-normal mb-1">{{this.productPrice}}</p>
            </td>
            <td>
              <p class="fw-normal mb-1">{{this.productQuantity}}</p>
            </td>
            <td>
              <p class="fw-normal mb-1">{{this.subTotal}}</p>
            </td>


          </tr>
          {{/each}}
         
         
          <h4 id="status" style="text-align: center; font-weight: bold; font-family: cursive; display: block;">No Sale for today</h4>
         
          <tr>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td>
              <h5 style="width: 0; font-weight: bold; font-family: cursive; ">Total Revenue:</h5>
            </td>

            <td>

              <i  id="totals" style="font-size: 25px;  " class="bi bi-currency-rupee">{{total}}</i>
   
    </td>
    </tr>
    </tbody>
 </div>
    </table>
    </div>
