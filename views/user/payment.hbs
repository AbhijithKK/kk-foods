      <div class="col-md-6">
        <div class="card mb-4">
          <div class="card-header py-3">
            <h5 class="mb-0">Delivery address</h5>
          </div>
          <div class="card-body">


            <!-- Single item -->
            <div class="row">
              <div class="card mb-2" style="width: 100%;">
                <div class="card-body">

                  <div class="row">

                    <div style="text-align: left;" class="col-sm-8">

                      <textarea id="textShow" style="border-color: #f5f7f62e;" name="address" placeholder="Add address"
                        rows="5" cols="50" readonly required>

                           </textarea>

                    </div>
                    {{!-- popwindow --}}
                    <div id="popWindow" class="card mb-2"
                      style="background-color: rgb(189, 212, 141); margin-top: -115px;z-index: 1;display: none;width: 400px;">


                    </div>
                    <div class="col-sm-4">
                      <a href="/editAddress"><i style="margin-top: 10px;" class="bi bi-pencil-square pdtDelete"></i></a>
                    </div>

                  </div>

                  <button id="addAddorChange" class="btn btn-primary">Add Address</button>



                </div>

              </div>

              <div class="card-body">
                <p><strong>Payment method</strong></p>

                <div style="    font-size: 17px;text-align: left;">
                  <form name="radiobtns">
                    <input id="cashOnDelivery" type="radio" name="payment" value="cod" checked>
                    <label for="html">Pay on Delivery</label><br>
                    <input type="radio" id="onlinePayment" name="payment" value="online">
                    <label for="css">Online payment</label>
                  </form>
                </div>
              </div>

            </div>
            <!-- Single item -->
          </div>
        </div>



